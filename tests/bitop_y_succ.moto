${
	byte one=1;
	byte sixteen=16;
	byte two=2;
	byte three=3;
	byte five=5;
	byte seven=7;
}$
$(one<<0) = 1
$(one<<1) = 2
$(one<<2) = 4
$(one<<3) = 8
$(one<<4) = 16
$(one<<5) = 32
$(one<<6) = 64
$(one<<7) = -128
$(one<<8) = 0
$(sixteen>>1) = 8 
$(sixteen>>2) = 4
$(sixteen>>3) = 2
$(sixteen>>4) = 1
$(~one) = -2
$(~(~one)) = 1
$(three & two) = 2
$(five & two) = 0
$(five | two) = 7
$(seven | two) = 7
$(five ^ two) = 7
$(seven ^ two) = 5
